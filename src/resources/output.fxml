<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="mainBorderPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.outputController">
   <top>
      <MenuBar fx:id="mainMenuBar" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#openTemplateC" text="open template" />
                  <Menu mnemonicParsing="false" text="save template">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#saveOriginGenC" text="original generation" />
                        <MenuItem mnemonicParsing="false" onAction="#saveCurrentGenC" text="current generation" />
                    </items>
                  </Menu>
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <Menu mnemonicParsing="false" text="switch">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#switchOriginalC" text="switch original" />
                      <MenuItem mnemonicParsing="false" onAction="#switchCurrentC" text="switch current" />
                        <MenuItem mnemonicParsing="false" onAction="#switchNewC" text="switch new" />
                    </items>
                  </Menu>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <left>
      <VBox fx:id="buttonVbox" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Label" />
            <Button fx:id="booleanBox" mnemonicParsing="false" onMouseClicked="#booleanBoxC" text="False">
               <VBox.margin>
                  <Insets left="25.0" right="25.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Label text="Label" />
            <TextField fx:id="aliveNeighbor" maxHeight="6.0" maxWidth="100.0" minHeight="20.0" onKeyTyped="#aliveNC" prefHeight="6.0" prefWidth="100.0" promptText="alive neighbor">
               <VBox.margin>
                  <Insets left="3.0" right="3.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <Label text="Label" />
            <TextField fx:id="deadNeighbor" maxHeight="20.0" minHeight="20.0" onKeyTyped="#deadNC" promptText="dead neighbor">
               <VBox.margin>
                  <Insets left="3.0" right="3.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="runButton" mnemonicParsing="false" onMouseClicked="#runButtonC" text="Start">
               <VBox.margin>
                  <Insets left="25.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="resetButton" mnemonicParsing="false" onMouseClicked="#resetButtonC" text="Reset">
               <VBox.margin>
                  <Insets left="24.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </left>
   <center>
      <StackPane fx:id="subScenePane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
